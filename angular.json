{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dashbord": {
      "projectType": "application",
      "schematics": {},
      
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            
            "outputPath": "dist/dashbord",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "baseHref": "/",
            "polyfills": [
              "zone.js"
            ],
           
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              "src/favicon"
            ],
            "styles": [
              
              "src/styles.css",
            
              "src/assets/css/dataTables.dataTables.css",
              "src/assets/css/buttons.dataTables.css",
              "src/assets/vendors/dropify/dist/dropify.min.css",
              "src/assets/vendors/dropzone/dropzone.min.css",
              "src/assets/vendors/select2/select2.min.css",
              "src/assets/libs/simplebar/dist/simplebar.css"

            ],
            "scripts": [
              "src/assets/js/jquery-3.7.1.min.js",
              "src/assets/js/jquery-3.7.1.js", 
              "src/assets/vendors/datatables.net/jquery.dataTables.js",
              "src/assets/js/dataTables.js",
              "src/assets/js/dataTables.buttons.js",
              "src/assets/js/buttons.dataTables.js",
              "src/assets/js/jszip.min.js",
              "src/assets/js/pdfmake.min.js",
              "src/assets/js/vfs_fonts.js",
              "src/assets/js/buttons.html5.min.js",
              "src/assets/js/buttons.print.min.js",
              "src/assets/vendors/datatables.net-bs5/dataTables.bootstrap5.js",
              "src/assets/js/select2.js",
              "src/assets/vendors/select2/select2.min.js",
              "src/assets/vendors/dropzone/dropzone.min.js",
              "src/assets/js/dropzone.js",
              "src/assets/vendors/dropify/dist/dropify.min.js",
              "src/assets/js/dropify.js"
            ],
            "server": "src/main.server.ts"
           
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "15kb"
                } 
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "dashbord:build:production"
            },
            "development": {
              "buildTarget": "dashbord:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "cli": {
    "analytics": "f58bd458-1f8f-4a7f-918e-afd8c2ef1b9f"
  }
  
}
